TEMPLATE = app
TARGET = iRIC
CONFIG += qt
CONFIG += debug_and_release

include( ../../paths.pri )

QT += widgets network
RC_FILE = iricgui.rc

######################
# Internal libraries #
######################

#iricGui library

win32 {
	CONFIG(debug, debug|release) {
		LIBS += -L"../../libs/gui/debug"
	} else {
		LIBS += -L"../../libs/gui/release"
	}
}
unix {
	LIBS += -L"../../libs/gui"
}
LIBS += -liricGui

#iricMisc library

win32 {
	CONFIG(debug, debug|release) {
		LIBS += -L"../../libs/misc/debug"
	} else {
		LIBS += -L"../../libs/misc/release"
	}
}
unix {
	LIBS += -L"../../libs/misc"
}
LIBS += -liricMisc

######################
# External libraries #
######################

# VTK

LIBS += \
	-lvtkCommonCore-8.2 \
	-lvtkCommonDataModel-8.2 \
	-lvtkInteractionStyle-8.2 \
	-lvtkIOExport-8.2 \
	-lvtkRenderingFreeType-8.2 \
	-lvtkRenderingOpenGL2-8.2

# Post-Build Event
win32 {
	QMAKE_POST_LINK += cd $$PWD/../../tools & copydlls.py
}

